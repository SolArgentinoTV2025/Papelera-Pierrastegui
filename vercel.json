{
    "cleanUrls": true,
    "trailingSlash": false,
    "redirects": [
      {
        "source": "/:path*",
        "destination": "/:path*",
        "permanent": true
      }
    ],
    "builds": [
      {
        "src": "**/*.html",
        "use": "@vercel/static"
      }
    ],
    "routes": [
      {
        "src": "/(.*)",
        "dest": "/$1"
      }
    ]
  }  